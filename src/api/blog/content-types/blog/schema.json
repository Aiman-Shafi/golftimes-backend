{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "blocks"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Tech",
        "Politics",
        "Others"
      ]
    },
    "Excerpt": {
      "type": "text"
    },
    "Tags": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    }
  }
}
